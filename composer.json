	"keywords":["readmoo","ecrowdmedia","codeigniter","aws","sdk"],
	"homepage":"https://readmoo.com/",
	"license":"proprietary",
	"authors":[
		{
			"name":"Willy",
			"email":"willy@ecrowdmedia.com",
			"homepage":"https://github.com/WillyYeh",
			"role":"Developer"
		}
	],
	"support": {
		"email":"tech@readmoo.com"
	},
	"require": {
		"php": "^8.0",
		"composer/installers": "^1.2",
