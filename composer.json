		"ps/image-optimizer": "^1.0.6",
		"ecrowdmedia/common": "^1.65.0"
	},
    "require-dev": {
        "rector/rector": "^1.2",
        "phpstan/phpstan": "^1.12"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:eCrowdMedia/Common.git"
        }
    ],
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
